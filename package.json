{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "set-db-url": "export DATABASE_URL=postgresql://testuser:testpassword@localhost:5432/testdb && echo $DATABASE_URL",
    "migrate:up": "npx kysely migrate:up",
    "migrate:down": "npx kysely migrate:down",
    "seed:run": "npx kysely seed:run",
    "prune-containers": "docker container prune -f",
    "start-postgres-container": "docker run --name my-postgres-container -p 5432:5432 my-postgres-test-db",
    "start:db": "npm run prune-containers && npm run start-postgres-container",
    "e2e": "export DATABASE_URL=postgresql://testuser:testpassword@localhost:5432/testdb  && npx kysely migrate:up && cypress open",
    "e2e:v2": "npx kysely migrate:up && cypress open",
    "e2e:run": "npx kysely migrate:up && cypress run --spec 'cypress/e2e/3-my-test/all.cy.js'"
  },
  "dependencies": { 
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "bcrypt": "^5.1.1",
    "cypress": "^13.13.3",
    "dotenv": "^16.4.5",
    "kysely": "^0.27.4",
    "kysely-ctl": "^0.9.0", 
    "kysely-postgres-js": "^2.0.0",
    "next": "14.2.5",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
